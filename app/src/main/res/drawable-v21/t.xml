<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="20dp"
            android:height="20dp"
            android:viewportWidth="20"
            android:viewportHeight="20">
            <path
                android:name="path"
                android:pathData="M 0 2.5 L 20 2.5"
                android:strokeColor="#e3341d"
                android:strokeWidth="5"
                android:trimPathEnd="0"/>
            <path
                android:name="path_1"
                android:pathData="M 10 5 L 10 12"
                android:strokeColor="#e3341d"
                android:strokeWidth="4"
                android:trimPathEnd="0"/>
            <path
                android:name="path_2"
                android:pathData="M 10 20 L 10 15"
                android:strokeColor="#e3341d"
                android:strokeWidth="4"
                android:trimPathEnd="0"/>
            <path
                android:name="path_3"
                android:pathData="M 2 1 L 18 1 M 2.5 4 L 2.5 7 M 17.5 0 L 17.5 3"
                android:strokeColor="#ffffff"
                android:strokeWidth="2"/>
            <path
                android:name="path_4"
                android:pathData="M 7 10 L 9.5 10 M 13 17 L 10.5 17"
                android:strokeColor="#ffffff"
                android:strokeWidth="1"/>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:duration="2000"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="path_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:duration="2000"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="path_2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:duration="2000"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
